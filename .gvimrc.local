syntax enable
set background=dark
colorscheme solarized
set guifont=Menlo\ for\ Powerline:h15
set lines=58
set columns=108


set cursorline                "highlight current line
set cursorcolumn              "highlight current column
set nobackup
set nowritebackup
set noswapfile
set nowrap
set number
set hlsearch
set transparency=10

