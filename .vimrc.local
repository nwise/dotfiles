syntax enable
colorscheme solarized
set background=dark
set cursorline                "highlight current line
set cursorcolumn              "highlight current column
set nobackup
set nowritebackup
set noswapfile
set nowrap
set number
set hlsearch
set guifont=Menlo\ for\ Powerline:h15
set listchars=tab:.>,trail:.
set list
set expandtab ts=2 sw=2 ai
let mapleader = ","
