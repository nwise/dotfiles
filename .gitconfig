[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
[user]
  name = Nathan Wise
  email = nathanwise@gmail.com
[merge]
  tool = p4mergetool
[mergetool "p4mergetool"]
  cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge $PWD/$BASE $PWD/$REMOTE $PWD/$LOCAL $PWD/$MERGED
  trustExitCode = false
[mergetool]
  keepBackup = false
[diff]
  tool = p4mergetool
[difftool "p4mergetool"]
  cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge $LOCAL $REMOTE
[core]
  editor = /usr/bin/vim
  excludesfile = /Users/nwise/.gitignore_global
[difftool "sourcetree"]
  cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
  trustExitCode = true
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[forever "tools"]
  base = /Users/nwise/workspace/forever/forever-tools
  hooks = /Users/nwise/workspace/forever/forever-tools/git-template/hooks
[include]
  path = /Users/nwise/workspace/forever/forever-tools/gitconfig.include
[init]
  templatedir = /Users/nwise/workspace/forever/forever-tools/git-template
